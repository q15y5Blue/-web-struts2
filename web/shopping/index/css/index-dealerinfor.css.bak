*{margin:0;padding:0;list-style:none;text-decoration:none;}
#header{clear:both;width:100%;height:30px;line-height:30px;background:#F1F1F1;}
a{color:#666;}
a:hover{color:red;}
body{font: 12px/150% Arial,Verdana,Microsoft YaHei,"\5b8b\4f53";color: #666;}
.dealer-infor{margin-top: 10px;}
.crumb{ height: 32px;line-height: 32px; font-family: simsun;}
.dealer-s-list{ text-align:right; padding-right: 10px;}
.dealer-b-list{ padding-right: 0;}
.c-style,.o-num{ height: 25px; line-height: 25px; border-radius: 2px;border: 1px solid #ccc;}
.c-laboratory{ width: 150px;}
.dealer-left{ float: left;}
.dealer-right{float: right;}
.k-textbox{ height: 25px; line-height: 25px; border-radius: 2px; border: 1px solid #ccc;}
.k-button{padding: 5px 4px;background-color: #C81624;border: 1px solid #C81624;
color: #fff;border-radius: 2px;}
.dealer-s-table{ margin-top: 10px;}
#table{ width: 100%; border-collapse:collapse; font-size: 12px;}
#table td{border: 1px solid #ccc;height:34px; line-height:34px;}
.tcdPageCode{text-align: left;color: #ccc; float: left;}
.tcdPageCode a{display: inline-block;color: #C81624;display: inline-block;height: 25px;line-height: 25px;padding: 0 10px;border: 1px solid #ddd;margin: 0 2px;border-radius: 4px;vertical-align: middle;}
.tcdPageCode a:hover{text-decoration: none;border: 1px solid #C81624;}
.tcdPageCode span.current{display: inline-block;height: 25px;line-height: 25px;padding: 0 10px;margin: 0 2px;color: #fff;background-color: #C81624;border: 1px solid #C81624;border-radius: 4px;vertical-align: middle;}
.tcdPageCode span.disabled{display: inline-block;height: 25px;line-height: 25px;padding: 0 10px;margin: 0 2px;color: #bfbfbf;background: #f2f2f2;border: 1px solid #bfbfbf;border-radius: 4px;vertical-align: middle;}

.k-title{ float:right; padding-right:10px; }
.b-date{ height: 25px; line-height: 25px; border-radius: 2px; border:1px solid #ccc;}
.search-botton{border: 1px solid #B61D1D;color: #fff;background-color: #B61D1D;padding: 4px 4px; border-radius: 2px;}
.empty-botton{ background-color:#fff; color: #666;border: 1px solid #ccc;border-radius:2px;}

.comment-box{border: 1px solid #c8c8c8;}
.comment-nav{background: #f0f0f0; overflow: hidden;}
.comment-nav h2{color: #4c4c4c;font-weight: normal;height: 32px;line-height: 32px; padding-left: 12px; float: left;}
.comment-nav-a{ float:right;height: 32px;line-height: 32px;color: #ff6600; padding-right: 12px;}
.comment-nav-a img{ width: 16px;}
.confirm-cont{border: 3px solid #ff9900;min-height: 485px; padding: 30px;}
.confirm-fillin h3{ font-weight:bold; color: #000; }
.confirm-fillin-shop h3{font-weight:bold; color: #000;}
.remittance-mode h3{font-weight:bold; color: #000;}
.confirm-fillin-left,.confirm-fillin-right{float: left; font-size: 14px;}
.confirm-fillin-left{ width: 9%; margin-right: 15px;}
.confirm-fillin-right{ width: 89%;}
.confirm-fillin{ border-bottom: 1px  solid #ccc; overflow: hidden; padding: 12px 0 30px 0;}
.fillin-txt-left{ height: 60px; line-height: 60px;}
.fillin-right-txt{height: 60px; line-height: 22px;}
.enclosure{padding-top: 10px;}
.g-date{ padding-top:5px; }
.search-box{border: 1px solid #dadada;height: 22px; line-height: 22px;}
.fillin-right-select{padding: 20px 0;}
.s-width{ width: 200px;border: 1px solid #dadada; height: 22px; line-height: 22px;}
.ui_button{ background-color: #C81624;color: #fff;padding: 5px 20px;border-radius: 2;}
.star{ color:#FF0014;; display: inline-block; margin-left: 5px; font-size: 18px;}
.file-txt{ font-size: 12px; display: inline-block; margin-left: 10px;}
.confirm-fillin-shop{ margin-top: 10px;}
.upload{ text-align: right; margin: 10px 0;}
.upload-button{border: 1px solid #ccc;border: 1px solid #ccc;background: #fff;padding: 2px 4px;}

.total-quantity{ text-align: right; font-size: 18px; margin: 10px 0;}
.total-color{ color: #ff9900;}
.mode-list{margin: 20px auto;padding: 5px 0;width: 500px;}
.mode-w{ width: 150px; display: inline-block; text-align: right; margin-right: 10px;}
.mode-next{ text-align: center;}
.next-botton{border: 1px solid #C81624; display: inline-block; width: 70px; height: 28px; line-height: 28px; cursor:pointer;color: #C81624; border-radius: 2px;}
